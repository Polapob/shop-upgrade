<tr id="<%= dom_id user %>">
  <td>
    <%= user.id %>
  </td>
  <td>
    <%= user.name %>
  </td>
   <td>
    <%= user.email %>
  </td>
  <td>
    <%= user.user_type %>
  </td>
  <%if user.id != current_user.id %>
  <td class="">
    <%= button_to "Delete", delete_user_path(:id => user.id) ,class:"btn btn-danger",method: :delete %>
  </td>
  <% end %>
</tr>
